{
  "name": "my-portfolio",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.json  ./src/assets/i18n/de.json --key-as-default-value --replace --format json",
